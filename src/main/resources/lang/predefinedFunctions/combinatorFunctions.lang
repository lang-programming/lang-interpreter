## Combinator execution: a(c)(b)
function combinator combC($a{FUNCTION_POINTER}, $b, $c) {
    $ret = $a($c)

    if(!($ret ~~ $LANG_TYPE_FUNCTION_POINTER)) {
        throw $LANG_ERROR_INVALID_FUNC_PTR, "The value returned by a(c) must be of type FUNCTION_POINTER"
    }

    return $ret($b)
}

